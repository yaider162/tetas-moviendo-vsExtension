{
  "name": "tetas-moviendo",
  "displayName": "tetas_moviendo",
  "description": "Extension que muestra tetas moviendose en el editor",
  "version": "0.0.1",
  "author": {"name": "Bombondecanela"},
  "repository": {
    "type": "git",
    "url": "https://github.com/yaider162/tetas-moviendo-vsExtension"
  },
  "engines": {"vscode": "^1.101.0"},
  "categories": ["Themes"],
  "activationEvents": ["onStartupFinished"],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "prueba",
        "title": "Tiempo actual"
      },
      {
        "command": "incrementar_contador",
        "title": "Incrementar contador"
      },
      {
        "command": "show_gif",
        "title": "Mostrar GIF"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.101.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2"
  }
}
